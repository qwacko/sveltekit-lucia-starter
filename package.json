		"eslint-plugin-svelte": "^2.43.0",
		"postcss": "^8.4.45",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "5.0.0-next.126",
		"svelte-check": "^4.0.0",
		"sveltekit-sse": "^0.13.5",
		"sveltekit-superforms": "^2.17.0",
