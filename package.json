{
	"name": "sveltekit-lucia-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:generate": "pnpm drizzle-kit generate:sqlite",
		"db:studio": "pnpm drizzle-kit studio",
		"generate-pwa-assets": "pwa-assets-generator"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.105",
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.23.0",
		"@types/better-sqlite3": "^7.6.4",
		"@types/node-schedule": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"@vite-pwa/assets-generator": "^0.0.8",
		"@vite-pwa/sveltekit": "^0.2.5",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"sveltekit-superforms": "^1.6.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"unplugin-icons": "^0.16.5",
		"vite": "^4.4.9",
		"vite-plugin-pwa": "^0.16.4",
		"vitest": "^0.34.2",
		"workbox-window": "^7.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-sqlite": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"better-sqlite3": "^8.5.2",
		"drizzle-orm": "^0.28.5",
		"lucia": "^2.4.0",
		"node-schedule": "^2.1.1",
		"zod": "^3.22.2"
	}
}
