{
	"name": "sveltekit-lucia-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:generate": "pnpm drizzle-kit generate:sqlite",
		"db:studio": "pnpm drizzle-kit studio",
		"db:custom": "pnpm drizzle-kit generate:sqlite --custom",
		"db:undo": "pnpm drizzle-kit drop",
		"generate-pwa-assets": "pwa-assets-generator"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.208",
		"@playwright/test": "^1.44.0",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/better-sqlite3": "^7.6.10",
		"@types/node-schedule": "^2.1.7",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"@vite-pwa/assets-generator": "^0.2.4",
		"@vite-pwa/sveltekit": "^0.5.0",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.21.1",
		"eslint": "^9.2.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "5.0.0-next.126",
		"svelte-check": "^3.7.1",
		"sveltekit-sse": "^0.13.0",
		"sveltekit-superforms": "^2.13.1",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unplugin-icons": "^0.19.0",
		"unstorage": "^1.10.2",
		"vite": "^5.2.11",
		"vite-plugin-pwa": "^0.20.0",
		"vitest": "^1.6.0",
		"workbox-window": "^7.1.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"@lucia-auth/adapter-sqlite": "^3.0.1",
		"@sveltejs/adapter-node": "^5.0.1",
		"better-sqlite3": "^9.6.0",
		"drizzle-orm": "^0.30.10",
		"lucia": "^3.2.0",
		"nanoid": "^5.0.7",
		"node-schedule": "^2.1.1",
		"oslo": "^1.2.0",
		"skguard": "^0.1.1",
		"skroutes": "^0.1.1",
		"zod": "^3.23.8"
	}
}
